<header class="navbar">
  <div class="header_links">
		<%= link_to "Home", root_url %> |
		<%= link_to "Events", events_path %> |
		<% if logged_in? %>
			<%= link_to "My Profile", current_user %> |
			<%= link_to "Log out", logout_path, method: :delete %>
		<% else %>
			<%= link_to "Sign up", signup_path %> |
			<%= link_to "Log in", login_path %>
		<% end %>
	</div>
</header>
