<h1>Welcome, <%= current_user.username %>!</h1>

<h3>Events you created:</h3>
<% if @user.organized_events.any? %>
  <ol class="events">
    <%= render @organized_events %>
  </ol>
<% end %>
<%= link_to "Create a new event", new_event_path %><br>
<%= link_to "See all events", events_path %><br>
<%= link_to "Log out", logout_path, method: :delete %>
